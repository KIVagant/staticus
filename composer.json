{
  "name": "develop/staticus",
  "type": "project",
  "license": "Commercial",
  "repositories": [
    {
      "type": "vcs",
      "url":  "https://github.com/newage/AudioManager.git"
    }
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev",
      "dev-develop": "1.1-dev"
    }
  },
  "require": {
    "newage/AudioManager": "0.0.1",
    "vlucas/phpdotenv": "2.2",
    "php": "^5.5 || ^7.0",
    "roave/security-advisories": "dev-master",
    "zendframework/zend-expressive": "^1.0",
    "zendframework/zend-expressive-helpers": "^2.0",
    "zendframework/zend-stdlib": "~2.7",
    "zendframework/zend-expressive-fastroute": "^1.0",
    "aura/di": "^3.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "squizlabs/php_codesniffer": "^2.3",
    "filp/whoops": "^1.1 || ^2.0"
  },
  "autoload" : {
    "files": [
      "library/Common/Config/env.php",
      "library/Common/Debug/debug.php"
    ],
    "psr-4": {
      "App\\": "src/App/",
      "Common\\": "src/Common/",
      "Staticus\\": "src/Staticus/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AppTest\\": "test/AppTest/"
    }
  }
}
